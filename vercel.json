{
  "rewrites": [
    { "source": "/api/:path*", "destination": "https://attractive-kindness-rbe-serveurs.up.railway.app/api/:path*" },
    { "source": "/auth/:path*", "destination": "https://attractive-kindness-rbe-serveurs.up.railway.app/api/auth/:path*" },
    { "source": "/events/:path*", "destination": "https://attractive-kindness-rbe-serveurs.up.railway.app/api/events/:path*" },
    { "source": "/vehicles/:path*", "destination": "https://attractive-kindness-rbe-serveurs.up.railway.app/api/vehicles/:path*" },
    { "source": "/newsletter/:path*", "destination": "https://attractive-kindness-rbe-serveurs.up.railway.app/api/newsletter/:path*" },
    { "source": "/finance/:path*", "destination": "https://attractive-kindness-rbe-serveurs.up.railway.app/api/finance/:path*" },
    { "source": "/documents/:path*", "destination": "https://attractive-kindness-rbe-serveurs.up.railway.app/api/documents/:path*" },
    { "source": "/members/:path*", "destination": "https://attractive-kindness-rbe-serveurs.up.railway.app/api/members/:path*" },
    { "source": "/site-users/:path*", "destination": "https://attractive-kindness-rbe-serveurs.up.railway.app/api/site-users/:path*" },
    { "source": "/changelog/:path*", "destination": "https://attractive-kindness-rbe-serveurs.up.railway.app/api/changelog/:path*" },
    { "source": "/flashes/:path*", "destination": "https://attractive-kindness-rbe-serveurs.up.railway.app/api/flashes/:path*" },
    { "source": "/stocks/:path*", "destination": "https://attractive-kindness-rbe-serveurs.up.railway.app/api/stocks/:path*" },
    { "source": "/public/:path*", "destination": "https://attractive-kindness-rbe-serveurs.up.railway.app/api/public/:path*" },
    { "source": "/((?!api/.*).*)", "destination": "/index.html" }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist"
}